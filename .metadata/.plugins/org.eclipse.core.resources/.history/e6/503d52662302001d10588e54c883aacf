package entities;

public class CountBank {

	private int number;
	private String holder;
	private double balance;
	
	public CountBank() {
	}
	
	
	
	
	
	public void deposit(double balance) {
		this.balance += balance;
	}

	public void whithdraw(double balance) {
		this.balance -= balance - 5 ;	
	}
	
}
